{cells:[{kind:2,language:'typescriptreact',value:'import { useState } from "react";\n\ntype ButtonProps = {\n  state: "primary" | "secondary" | "danger";\n};\n\nconst Button = ({ state }: ButtonProps) => {\n  const [count, setCount] = useState(0);\n\n  const backgroundColor = {\n    primary: "green",\n    secondary: "lightgray",\n    danger: "lightgray",\n  }[state];\n\n  const color = {\n    primary: "white",\n    secondary: "black",\n    danger: "red",\n  }[state];\n\n  return (\n    <button\n      style={{ padding: "10px", borderRadius: "5px", color, backgroundColor }}\n      onClick={() => setCount((c) => c + 1)}\n    >\n      I\'ve been clicked {count} times\n    </button>\n  );\n};\n',metadata:{id:'1FtKnLhrs6CYnTrYsKHeE'}},{kind:2,language:'typescriptreact',value:'<Button state="primary" />;\n',metadata:{id:'9xdtSucCgh7wzqS4Ix2Gj'}},{kind:2,language:'typescriptreact',value:'<Button state="secondary" />;\n',metadata:{id:'6jjyvQQ5m22t36q9hHolO'}},{kind:2,language:'typescriptreact',value:'<Button state="danger" />;\n',metadata:{id:'Bd1xmiQ8uv05PFD0aINRs'}}]}